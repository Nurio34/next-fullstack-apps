{
    "version": 2,
    "builds": [
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        },
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["prisma/mongo.prisma", ".env"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
